import { TypeOrmModule } from '@nestjs/typeorm';
import { Woman } from './women/woman.entity';

@Module({
  imports: [
    TypeOrmModule.forRoot({
      type: 'mysql',
      host: 'localhost',
      port: 3306,
      username: 'root',
      password: 'password',
      database: 'women_db',
      entities: [Woman],
      synchronize: true,
    }),
    TypeOrmModule.forFeature([Woman]),
  ],
  ...
})
